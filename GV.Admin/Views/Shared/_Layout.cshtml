    <script src="~/app/assets/plugins/angular-ckeditor/ckeditor/ckeditor.js"></script>
    <script src="~/app/assets/plugins/angular-ckeditor/angular-ckeditor.min.js"></script>

    <script src="~/app/assets/plugins/angular-hotkeys/hotkeys.min.js"></script>

    <script src="~/app/assets/plugins/angular-slimscroll/angular-slimscroll.js"></script>

    <script src="~/app/assets/plugins/angular-toastr/angular-toastr.tpls.js"></script>
    <!-- END PAGE LEVEL PLUGINS -->
    <!-- BEGIN THEME GLOBAL SCRIPTS -->
    <script src="~/app/services/authInterceptorService.js"></script>
    <script src="~/app/assets/m4.7/global/scripts/app.js"></script>
    <!-- END THEME GLOBAL SCRIPTS -->
    <!-- BEGIN PAGE LEVEL SCRIPTS -->
    <script src="~/app/services/alertService.js"></script>
    <script src="~/app/services/xhttpService.js"></script>
    <script src="~/app/services/gv.modal.js"></script>
    <script src="~/app/services/authService.js"></script>

    <script src="~/app/directives/ngEnter.js"></script>

    <script src="~/app/app.js"></script>
    <script src="~/app/services/sideBarMenuSvc.js"></script>
    <script src="~/app/components/core/core.js"></script>
    <script src="~/app/services/gv.rest.category.js"></script>
    <script src="~/app/services/gv.rest.article.js"></script>
    <script src="~/app/services/gv.rest.fsSvc.js"></script>
    <script src="~/app/services/gv.rest.setting.js"></script>
    @RenderSection("js", required: false)
    <!-- END PAGE LEVEL SCRIPTS -->
    <!-- BEGIN THEME LAYOUT SCRIPTS -->
    <script src="~/app/assets/m4.7/layouts/layout/scripts/layout.js"></script>
    <!-- END THEME LAYOUT SCRIPTS -->
    <!-- END JAVASCRIPTS -->

</body>

<!-- END BODY -->
</html>
